            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "husky": "^4.3.8",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.1",
        "stylelint": "^13.13.1",
        "stylelint-config-standard": "^22.0.0"
    }
